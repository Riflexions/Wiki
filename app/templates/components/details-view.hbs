<ul class="list-group-item-heading">
    {{#each data.query.search as |value|}}
    <li class="list-group-item">
        <h4 class="list-group-item-heading">{{value.title}}</h4>
        <br> {{{value.snippet}}}
        <br>
        <button {{action 'searchDetails' value.title}} class="btn btn-default">Read More</button>
    </li>
    {{/each}}
</ul>
